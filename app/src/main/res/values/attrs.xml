<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
    <declare-styleable name="SwipeRefreshLayout">
        <attr name="direction">
            <enum name="top" value="0" />
            <enum name="bottom" value="1" />
            <enum name="both" value="2" />
        </attr>
    </declare-styleable>

    <attr name="zxText" format="string" />
    <attr name="zxTextColor" format="color" />
    <attr name="zxTextSize" format="dimension" />

    <declare-styleable name="CustomTitleView">
        <attr name="zxText" />
        <attr name="zxTextColor" />
        <attr name="zxTextSize" />
    </declare-styleable>
</resources>